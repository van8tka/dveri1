@model dveri1.Models.ModelArticles
@{
        Layout = null;
}
<div class="fll db">
    @if (Model.ArticlesList.Count() > 0)
    {
        foreach (var i in Model.ArticlesList)
        {
            <div class="bootharticles fll">
                <a class="Linkarticleleftbooth" href="/Articles/GetArticle?id=@i.ID">
                    <h4>@i.Name</h4>
                    <span>@i.Headings</span>
                </a>
                </div>
        }
    }
</div>